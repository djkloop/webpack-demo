  },
  "homepage": "https://github.com/djkloop/webpack-demo#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefix": "^1.0.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
